# New-Placeholder "EETT_Dossier_GreekTrustCo_2025-06-15.asice\02a_Cover_Letter_Electronic_System_Access_SIGNED.pdf"
# New-Placeholder "EETT_Dossier_GreekTrustCo_2025-06-15.asice\06_Test_Certificates\TSA_CA_Certificate.pem"

# 4) Optional: compute SHA-256 to assist manifest generation
if (Get-Command "Get-FileHash" -ErrorAction SilentlyContinue) {
  Write-Host "[INFO] Computing SHA-256 digests (excluding META-INF/*)"
  $root = "EETT_Dossier_GreekTrustCo_2025-06-15.asice"
  $files = Get-ChildItem -Path $root -Recurse -File | Where-Object { $_.FullName -notmatch "\\META-INF\\" }
  $lines = foreach ($f in $files) {
    $h = Get-FileHash -Algorithm SHA256 -Path $f.FullName
    "{0}  .\{1}" -f $h.Hash.ToLower(), ($f.FullName -replace '^.+?EETT_Dossier_GreekTrustCo_2025-06-15\.asice\\','EETT_Dossier_GreekTrustCo_2025-06-15.asice\')
  }
  $lines | Out-File -FilePath "SHA256SUMS.txt" -Encoding ascii
  Write-Host "[INFO] SHA256SUMS.txt generated (template input for manifest.xml)"
} else {
  Write-Host "[WARN] Get-FileHash not available; skip digest generation"
}

# 5) Reminder for packaging & signing (performed outside this script)
Write-Host "[NEXT] Package ASiC-E with mimetype stored (no compression), add META-INF\manifest.xml, sign with XAdES-LTA."
Write-Host "[DONE] Template alignment complete."
